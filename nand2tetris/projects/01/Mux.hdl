/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    And(a=b, b=sel, out=out2);
    Not(in=sel, out=temp);
    And(a=a, b=temp, out=out1);
    Or(a=out2, b=out1, out=out);
}
